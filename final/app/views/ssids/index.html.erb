
<div class="wrapper">
<div class="header center">
<h1>mapwi</h1>
<%= tabs %>
</div> <!-- end header center -->
<div class="content">
<div id="introduction" class="txt">
<h1 class="center txt introDirective">Listing ssids
<br></br> <!--main listing here -->

<table>
  <thead>
    <tr>
      <th>Ssid</th>
      <th>Auth</th>
      <th>Encryption</th>
      <th>Radio type</th>
      <th>High signal</th>
      <th>High rssi</th>
      <th>Net type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ssids.each do |ssid| %>
      <tr>
        <td><%= ssid.ssid %></td>
        <td><%= ssid.auth %></td>
        <td><%= ssid.encryption %></td>
        <td><%= ssid.radio_type %></td>
        <td><%= ssid.high_signal %></td>
        <td><%= ssid.high_rssi %></td>
        <td><%= ssid.net_type %></td>
        <td><%= link_to 'Show', ssid %></td>
        <td><%= link_to 'Edit', edit_ssid_path(ssid) %></td>
        <td><%= link_to 'Destroy', ssid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ssid', new_ssid_path %>
</div> <!-- end buttonText -->
</div> <!-- end buttonContent -->
</div> <!-- end center pasteMsg -->
</div> <!-- end txt -->
</div> <!-- end content -->
</div> <!-- end wrapper-->

