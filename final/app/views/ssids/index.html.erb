<h1>Listing ssids</h1>

<table>
  <thead>
    <tr>
      <th>Ssid</th>
      <th>Bssid</th>
      <th>Manufacture</th>
      <th>Auth</th>
      <th>Encryption</th>
      <th>Sec type</th>
      <th>Radio type</th>
      <th>Channel</th>
      <th>Transfer rates</th>
      <th>Special transfer rates</th>
      <th>High signal</th>
      <th>High rssi</th>
      <th>Net type</th>
      <th>Label</th>
      <th>Public</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ssids.each do |ssid| %>
      <tr>
        <td><%= ssid.ssid %></td>
        <td><%= ssid.bssid %></td>
        <td><%= ssid.manufacture %></td>
        <td><%= ssid.auth %></td>
        <td><%= ssid.encryption %></td>
        <td><%= ssid.sec_type %></td>
        <td><%= ssid.radio_type %></td>
        <td><%= ssid.channel %></td>
        <td><%= ssid.transfer_rates %></td>
        <td><%= ssid.special_transfer_rates %></td>
        <td><%= ssid.high_signal %></td>
        <td><%= ssid.high_rssi %></td>
        <td><%= ssid.net_type %></td>
        <td><%= ssid.label %></td>
        <td><%= ssid.public %></td>
        <td><%= ssid.user %></td>
        <td><%= link_to 'Show', ssid %></td>
        <td><%= link_to 'Edit', edit_ssid_path(ssid) %></td>
        <td><%= link_to 'Destroy', ssid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ssid', new_ssid_path %>
