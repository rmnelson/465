<h1>Listing pollpoints</h1>

<table>
  <thead>
    <tr>
      <th>Ssid</th>
      <th>Gps</th>
      <th>Signal</th>
      <th>Rssi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pollpoints.each do |pollpoint| %>
      <tr>
        <td><%= pollpoint.ssid %></td>
        <td><%= pollpoint.gps %></td>
        <td><%= pollpoint.signal %></td>
        <td><%= pollpoint.rssi %></td>
        <td><%= link_to 'Show', pollpoint %></td>
        <td><%= link_to 'Edit', edit_pollpoint_path(pollpoint) %></td>
        <td><%= link_to 'Destroy', pollpoint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pollpoint', new_pollpoint_path %>
