<h1>Listing image_users</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>User</th>
      <th>Image</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_users.each do |image_user| %>
      <tr>
        <td><%= image_user.image_id %></td>
        <td><%= image_user.user_id %></td>
        <td><%= image_user.Image %></td>
        <td><%= image_user.User %></td>
        <td><%= link_to 'Show', image_user %></td>
        <td><%= link_to 'Edit', edit_image_user_path(image_user) %></td>
        <td><%= link_to 'Destroy', image_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image user', new_image_user_path %>
