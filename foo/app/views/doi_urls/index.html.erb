<h1>Listing doi_urls</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Name</th>
      <th>Description</th>
      <th>Doi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doi_urls.each do |doi_url| %>
      <tr>
        <td><%= doi_url.url %></td>
        <td><%= doi_url.name %></td>
        <td><%= doi_url.description %></td>
        <td><%= doi_url.doi %></td>
        <td><%= link_to 'Show', doi_url %></td>
        <td><%= link_to 'Edit', edit_doi_url_path(doi_url) %></td>
        <td><%= link_to 'Destroy', doi_url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doi url', new_doi_url_path %>
